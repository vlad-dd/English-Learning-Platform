    "@types/react-dom": "^18.0.6",
    "antd": "^4.23.1",
    "firebase": "^9.9.4",
    "framer-motion": "^7.3.5",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-quiz-component": "^0.5.1",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
